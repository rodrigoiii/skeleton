{
	"name": "rodrigoiii/slim-project",
	"description": "",
	"type": "project",
	"license": "MIT",
	"keywords": ["slim", "micro framework", "php"],
	"authors": [
	    {
	        "name": "Rodrigo Galura III",
	        "email": "rodrigogalura3rd@gmail.com"
	    }
	],
	"require": {
		"php": "^5.6.40 || ^7.0 || ^7.1",
		"illuminate/database": "^5.4",
		"php-di/slim-bridge": "^1.1",
		"slim/slim": "^3.12",
		"slim/twig-view": "^2.5",
		"vlucas/phpdotenv": "^3.3"
	},
	"autoload": {
	    "psr-4": {
	        "App\\": "app/src/"

            ,"SkeletonCore\\": "skeleton-core/src/classes"
	    },
        "files": [
            "skeleton-core/src/functions/autoload.php"
        ]
	},
	"scripts": {
	    "post-root-package-install": [
	        "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
	    ]
	},
	"config": {
	    "optimize-autoloader": true,
	    "sort-packages": true,
	    "platform": {
	        "php": "5.6.40"
	    }
	}
}
